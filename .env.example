# Rust log level (trace, debug, info, warn, error)
RUST_LOG=info

# For remote mode: Addresses of all parties (comma-separated)
ADDRESSES=127.0.0.1:7001,127.0.0.1:7002,127.0.0.1:7003

# Maximum database size (number of vectors)
MAX_DB=100000

# Number of sessions per request
SESSION_PER_REQUEST=4

# Connection parallelism
CONNECTION_PARALLELISM=1

# Request parallelism
REQUEST_PARALLELISM=1
